# Matchfile - fastlane match configuration
# Docs: https://docs.fastlane.tools/actions/match/

# Git repo to store certificates and profiles
git_url(ENV.fetch("MATCH_GIT_URL"))

# Storage type (git, google_cloud, s3)
storage_mode("git")

# Certificate type
type("developer_id")

# Read-only mode (set to false to generate new certs)
readonly(true)

# Platform
platform("macos")

# Developer ID doesn't use provisioning profiles
skip_provisioning_profiles(true)
